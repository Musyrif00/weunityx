{
  "expo": {
    "name": "WeUnityX",
    "slug": "weunityx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logowithname.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logosplash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logowithname.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logowithname.jpg"
    }
  }
}
